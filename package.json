{
  "name": "be-iqx",
  "version": "1.0.0",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "node test_api.js",
    "test-vnindex": "node test_vnindex.js",
    "setup": "node setup.js",
    "collect-tickers": "curl -X POST http://localhost:5001/api/admin/collect-data",
    "health": "curl http://localhost:5001/health",
    "stats": "curl http://localhost:5001/api/tickers/statistics",
    "historical-stats": "curl http://localhost:5001/api/historical-prices/statistics",
    "ownership-stats": "curl http://localhost:5001/api/ownership/statistics",
    "technical-stats": "curl http://localhost:5001/api/technical/statistics",
    "impact-stats": "curl http://localhost:5001/api/impact-index/statistics",
    "foreign-stats": "curl http://localhost:5001/api/foreign-trading/statistics",
    "collect-prices": "curl -X POST http://localhost:5001/api/historical-prices/collect-bulk -H 'Content-Type: application/json' -d \"{\\\"tickers\\\": $(cat tickers.json | tr -d '\\n'), \\\"type\\\": \\\"recent\\\"}\"",
    "collect-prices-full": "curl -X POST http://localhost:5001/api/historical-prices/collect-bulk -H 'Content-Type: application/json' -d \"{\\\"tickers\\\": $(cat tickers.json | tr -d '\\n'), \\\"type\\\": \\\"all\\\"}\"",
    "collect-ownership": "curl -X POST http://localhost:5001/api/ownership/collect -H 'Content-Type: application/json' -d \"{\\\"tickers\\\": $(cat tickers.json | tr -d '\\n')}\"",
    "collect-technical": "curl -X POST http://localhost:5001/api/technical/collect -H 'Content-Type: application/json' -d \"{\\\"tickers\\\": $(cat tickers.json | tr -d '\\n'), \\\"useWorkers\\\": true, \\\"workerCount\\\": 128}\"",
    "collect-technical-hour": "curl -X POST http://localhost:5001/api/technical/collect -H 'Content-Type: application/json' -d \"{\\\"tickers\\\": $(cat tickers.json | tr -d '\\n'), \\\"timeFrame\\\": \\\"ONE_HOUR\\\", \\\"useWorkers\\\": true, \\\"workerCount\\\": 128}\"",
    "collect-technical-day": "curl -X POST http://localhost:5001/api/technical/collect -H 'Content-Type: application/json' -d \"{\\\"tickers\\\": $(cat tickers.json | tr -d '\\n'), \\\"timeFrame\\\": \\\"ONE_DAY\\\", \\\"useWorkers\\\": true, \\\"workerCount\\\": 128}\"",
    "collect-technical-week": "curl -X POST http://localhost:5001/api/technical/collect -H 'Content-Type: application/json' -d \"{\\\"tickers\\\": $(cat tickers.json | tr -d '\\n'), \\\"timeFrame\\\": \\\"ONE_WEEK\\\", \\\"useWorkers\\\": true, \\\"workerCount\\\": 128}\"",
    "collect-impact-index": "curl -X POST http://localhost:5001/api/impact-index/collect",
    "collect-foreign-trading": "curl -X POST http://localhost:5001/api/foreign-trading/collect",
    "backup": "node tools/database-backup.js --full",
    "backup-schema": "node tools/database-backup.js --schema",
    "backup-data": "node tools/database-backup.js --data",
    "backup-all": "node tools/database-backup.js --all",
    "restore": "node tools/database-restore.js",
    "restore-latest": "node tools/database-restore.js --latest",
    "backup-manager": "node tools/backup-manager.js",
    "backup-list": "node tools/backup-manager.js list",
    "backup-cleanup": "node tools/backup-manager.js cleanup",
    "backup-stats": "node tools/backup-manager.js stats",
    "test-backup": "node test_backup_tools.js",
    "backup-nodejs": "node tools/nodejs-backup.js --data",
    "backup-nodejs-table": "node tools/nodejs-backup.js --table",
    "backup-test-connection": "node tools/nodejs-backup.js --test",
    "backup-simple": "node tools/simple-backup.js --full",
    "backup-compressed": "node tools/simple-backup.js --compressed",
    "backup-large": "node tools/simple-backup.js --large",
    "pm2:start": "./scripts/pm2-start.sh",
    "pm2:stop": "./scripts/pm2-stop.sh",
    "pm2:restart": "./scripts/pm2-restart.sh",
    "pm2:logs": "./scripts/pm2-logs.sh",
    "pm2:monitor": "./scripts/pm2-monitor.sh",
    "pm2:status": "pm2 status be-iqx",
    "convert-backup": "node tools/convert-backup-format.js",
    "convert-backup-all": "node tools/convert-backup-format.js --all"
  },
  "keywords": ["nodejs", "express", "postgresql", "stock-data", "api"],
  "author": "",
  "license": "ISC",
  "description": "Professional Node.js application for collecting stock ticker data from Simplize API",
  "dependencies": {
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
